{{#each categories}}
    {{!-- <div class="row"> --}}
        <div class="category_row">
            <div class="col-5 category_image">
                <img src={{imageUrl}} alt={{name}}/>
            </div>
            <div class="col-7 category_desc">
                <h3>{{name}}</h3>
                <p>{{description}}</p>
                <button>Explore {{key}}</button>
            </div>
        </div>
    {{!-- </div> --}}
{{/each}}